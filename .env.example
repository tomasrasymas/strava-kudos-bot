# Number of times to scroll to the bottom of the feed before refreshing
NUMBER_OF_SCROLLS_TO_END=5

# Interval in seconds between cycles (default: 3600 = 1 hour)
INTERVAL_SECONDS=3600

# Comma-separated list of athlete names to skip when giving kudos
# Use partial names - matching is case-insensitive
ATHLETES_TO_SKIP=YourName,AnotherAthlete

# Path where to store athelte activity map image
# If not set - image will not be stored
SAVE_MAP_PATH=data/maps
